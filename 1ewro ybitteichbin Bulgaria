        function tabcontent:Keybind(text, default, callback)
            local keybind = default or Enum.KeyCode.Unknown
            local Keybind = Instance.new("Frame")
            local KeybindCorner = Instance.new("UICorner")
            local KeybindTitle = Instance.new("TextLabel")
            local KeybindStroke = Instance.new("UIStroke")
            local KeybindBtn = Instance.new("TextButton")

            Keybind.Name = "Keybind"
            Keybind.Parent = Tab
            Keybind.BackgroundColor3 = CurrentUIBackgroundColor
            Keybind.Size = UDim2.new(0, 363, 0, 42)

            KeybindCorner.CornerRadius = UDim.new(0, 5)
            KeybindCorner.Parent = Keybind

            KeybindStroke.Parent = Keybind
            KeybindStroke.Thickness = 1
            KeybindStroke.Transparency = 0.3
            KeybindStroke.Color = Color3.fromRGB(0, 0, 0)
            KeybindStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

            KeybindTitle.Name = "KeybindTitle"
            KeybindTitle.Parent = Keybind
            KeybindTitle.BackgroundTransparency = 1
            KeybindTitle.Position = UDim2.new(0.036, 0, 0, 0)
            KeybindTitle.Size = UDim2.new(0, 187, 0, 42)
            KeybindTitle.Font = Enum.Font.Gotham
            KeybindTitle.Text = text or "Unnamed Keybind"
            KeybindTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            KeybindTitle.TextSize = 14
            KeybindTitle.TextXAlignment = Enum.TextXAlignment.Left

            KeybindBtn.Name = "KeybindBtn"
            KeybindBtn.Parent = Keybind
            KeybindBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            KeybindBtn.Position = UDim2.new(0.65, 0, 0.19, 0)
            KeybindBtn.Size = UDim2.new(0, 100, 0, 26)
            KeybindBtn.Font = Enum.Font.Gotham
            KeybindBtn.Text = default and default.Name or "None"
            KeybindBtn.TextColor3 = Color3.fromRGB(200, 200, 200)
            KeybindBtn.TextSize = 14
            KeybindBtn.AutoButtonColor = false

            local BtnCorner = Instance.new("UICorner")
            BtnCorner.CornerRadius = UDim.new(0, 4)
            BtnCorner.Parent = KeybindBtn

            local waitingForKey = false
            KeybindBtn.MouseButton1Click:Connect(function()
                KeybindBtn.Text = "..."
                waitingForKey = true
            end)

            UserInputService.InputBegan:Connect(function(input)
                if waitingForKey and input.UserInputType == Enum.UserInputType.Keyboard then
                    keybind = input.KeyCode
                    KeybindBtn.Text = input.KeyCode.Name
                    waitingForKey = false
                    pcall(callback, keybind)
                elseif input.KeyCode == keybind and not waitingForKey then
                    pcall(callback, keybind)
                end
            end)

            Tab.CanvasSize = UDim2.new(0, 0, 0, TabLayout.AbsoluteContentSize.Y)
        end

